    <link href="https://weixinote.com/2018/09/13/Go-%E8%AE%A1%E7%AE%97%E6%96%87%E4%BB%B6MD5/"/>
    <id>https://weixinote.com/2018/09/13/Go-计算文件MD5/</id>
